{
    "name": "orchestra/avatar",
    "description": "Avatar Component for Orchestra Platform",
    "keywords": ["orchestra-platform", "orchestral", "laravel", "avatar", "gravatar"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Avatar\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1",
        "orchestra/support-facades": "~3.7.0",
        "orchestra/support-providers": "~3.7.0"
    },
    "require-dev": {
        "illuminate/container": "~5.7.0",
        "illuminate/database": "~5.7.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.7-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
