{
    "name": "orchestra/avatar",
    "description": "Avatar Component for Orchestra Platform",
    "keywords": ["orchestra-platform", "orchestral", "laravel", "avatar", "gravatar"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Avatar\\": "src/"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "orchestra/support-facades": "~3.3.0",
        "orchestra/support-providers": "~3.3.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.1",
        "illuminate/container": "~5.3.0",
        "illuminate/database": "~5.3.0"
    },
    "suggest": {
        "laravel/framework": "Allow using orchestra/avatar component outside of Orchestra Platform (~5.3).",
        "orchestra/foundation": "Allow using orchestra/avatar with Orchestra Platform (~3.3)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.3-dev"
        }
    },
    "minimum-stability": "dev"
}
